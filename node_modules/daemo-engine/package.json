{
  "name": "daemo-engine",
  "version": "0.2.4",
  "description": "The official Daemo Engine SDK for Node.js to connect your services and build AI agents.",
  "main": "public/index.js",
  "types": "public/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && npm run copy-proto",
    "copy-proto": "mkdir -p public/proto && cp src/proto/daemo.proto public/proto/",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "start": "node public/app.js",
    "dev": "npx ts-node src/app.ts",
    "diagnostic": "npx ts-node src/diagnostic.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Daemo-ai/daemo-engine.git"
  },
  "keywords": [
    "daemo",
    "ai",
    "agent",
    "llm",
    "sdk",
    "typescript",
    "tools",
    "functions"
  ],
  "author": "Jimmy Carter, Sharmila Guntaka, Srikar Godilla",
  "license": "ISC",
  "type": "commonjs",
  "homepage": "https://app.daemo.ai",
  "files": [
    "public"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "google-protobuf": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "ssl-root-cas": "^1.3.1",
    "uuid": "^13.0.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    }
  },
  "optionalDependencies": {
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^24.9.1",
    "@types/uuid": "^10.0.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.13.0",
    "ts-json-schema-generator": "^2.4.0",
    "typescript": "^5.9.3"
  }
}
